<AnchorPane fx:id="Booking_pne" prefHeight="559.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.User.BookingController">
